{
  "name": "node",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "volta": {
    "node": "20.11.0",
    "pnpm ": "8.15.1"
  },
  "scripts": {
    "build": "rimraf dist && swc ./src -d dist",
    "db-seed": "npx prisma db seed",
    "docker:db:migrate:undo": "docker compose run backend npx knex migrate:down",
    "db:console": "docker compose exec db psql -h localhost -U postgres -d nodebackend_dev",
    "db:migrate": "knex migrate:latest",
    "docker:db:migrate": "docker compose run backend pnpm  db:migrate",
    "rebuild:backend": "docker compose build backend && docker compose rm --stop backend && docker compose up -d backend",
    "dev": "pnpm  build && nodemon dist/index.js",
    "dev:docker": "pnpm  build && node --inspect=0.0.0.0 dist/index.js",
    "checked": "ncu -u && pnpm  install",
    "prepare": "husky install",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "@types/uuid": "^9.0.8",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "husky": "^9.0.11",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "nanoid": "^5.0.7",
    "pg": "^8.12.0",
    "typescript": "^5.4.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.10",
    "@swc/core": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.0.2",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  }
}
